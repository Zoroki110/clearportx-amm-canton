# Production Environment Variables for ClearportX AMM
# These will be replaced with your ngrok URLs

# Backend API URL (from ngrok http 8080)
VITE_API_URL=https://YOUR_BACKEND_NGROK_URL.ngrok.io

# Keycloak Auth URL (from ngrok http 8082)
VITE_KEYCLOAK_URL=https://YOUR_KEYCLOAK_NGROK_URL.ngrok.io
VITE_KEYCLOAK_REALM=clearportx
VITE_KEYCLOAK_CLIENT_ID=clearportx-frontend

# Canton Network Configuration
VITE_CANTON_NETWORK=devnet
VITE_CANTON_PARTICIPANT_URL=https://YOUR_BACKEND_NGROK_URL.ngrok.io

# App Configuration
VITE_APP_NAME=ClearportX AMM
VITE_APP_VERSION=1.0.0
VITE_ENABLE_DEVNET=true

# Token Standards
VITE_CBTC_TOKEN_API=https://api.utilities.digitalasset-dev.com/api/token-standard/v0/registrars/cbtc-network::1220217a5a881a949203ff/registry/metadata/v1/instruments